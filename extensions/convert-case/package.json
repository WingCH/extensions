{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "convert-case",
  "title": "Convert Case",
  "description": "Convert strings between camelCase, PascalCase, Capital Case, snake_case and more",
  "icon": "command-icon.png",
  "author": "WingCH",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "convert-case",
      "title": "Convert Case",
      "description": "Enter or paste text",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.3",
    "change-case": "^4.1.2",
    "title-case": "^3.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "2.7.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
